! Font config

Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

*background: #000000
*foreground: #F2F3F4

!black
*color0:			#242424
*color8:			#000000
!red
*color1:			#FF0090
*color9:			#FF0000
!green
*color2:			#A7FC00
*color10:			#00FF00
!yellow->orange
*color3:			#FF5A36
*color11:			#FF4500
!blue
*color4:			#007BA7
*color12:			#1E90FF
!magenta
*color5:			#9966CC
*color13:			#8F00FF
!cyan->gray
*color6:		 	#91A3B0
*color14:			#536878
!white
*color7:			#F0EAD6
*color15:			#F8F8FF

! fonts
! run "fc-list" for a list of available fonts
!URxvt*font: xft:terminus-font:size-11
URxvt*font: xft:monospace:size=9
URxvt.letterSpace: -2

urxvt.termName: xterm-256color
urxvt.scrollBar: false
urxvt.imLocale: de_DE.UTF-8
urxvt.loginShell: true
urxvt.saveLines: 512
urxvt.geometry: 90x25
urxvt.boldMode: false
urxvt.inheritPixmap: false
urxvt.jumpScroll: true
urxvt.tintColor: white
URxvt*internalBorder: 3
URxvt*internalBordercolor: #121212
urxvt.cursorBlink: false
urxvt.cursorColor: #ff8939
urxvt.cursorColor2: #121212
urxvt.colorBD: #857672
urxvt.colorIT: #bbbbbb
urxvt.colorUL: #999999
urxvt.underlineColor: #999999
urxvt.borderless: true


