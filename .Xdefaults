! Mashup colors
*foreground:        #B2A191
*background:        #171717
*borderColor:       #171717
*cursorColor:       #C7A551
!black
*color0:            #202020
*color8:            #404040
!red
*color1:            #BF3F34
*color9:            #FF6C5F
!green
*color2:            #707D22
*color10:           #B8CA4B
!yellow
*color3:            #BF7A29
*color11:           #C7A551
!blue
*color4:            #627A92
*color12:           #95B9DE
!magenta
*color5:            #75507B
*color13:           #AD7FA8
!cyan
*color6:            #757978
*color14:           #9FA590
!white
*color7:            #B2A191
*color15:           #E8D0C3

URxvt*foreground:White
URxvt*background:Black

! fonts
! run "fc-list" for a list of available fonts
URxvt*font: xft:terminus-font:size-9

urxvt.termName: xterm-256color
urxvt.scrollBar: false
urxvt.imLocale: de_DE.UTF-8
urxvt.loginShell: true
urxvt.saveLines: 512
urxvt.geometry: 90x25
urxvt.boldMode: false
urxvt.inheritPixmap: false
urxvt.xftAntialias: false
urxvt.jumpScroll: true
urxvt.tintColor: white
URxvt*internalBorder: 3
URxvt*internalBordercolor: #121212
urxvt.cursorBlink: false
urxvt.cursorColor: #ff8939
urxvt.cursorColor2: #121212
urxvt.colorBD: #857672
urxvt.colorIT: #bbbbbb
urxvt.colorUL: #999999
urxvt.underlineColor: #999999
urxvt.borderless: true
